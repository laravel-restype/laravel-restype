    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRESType\\LaravelRESTypeServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "typescript",
        "type-safety",
        "tRPC",
        "RPC",
        "REST-api"
    ]
